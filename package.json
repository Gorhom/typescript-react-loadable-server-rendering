{
  "name": "react-loadable-ssr",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-iso-dev": "webpack -d --devtool source-map --config config/webpack.config.iso.js",
    "build-iso-prod": "npm run clean && webpack -p --config config/webpack.config.iso.js",
    "build-client-dev": "npm run clean-client && webpack -d --devtool source-map --config config/webpack.config.js",
    "build-server-dev": "npm run clean-server && npm run tsc && npm run babel && npm run clean-temp",
    "build-dev": "npm run build-client-dev && npm run build-server-dev",
    "build-client-prod": "npm run clean-client && webpack -p --config config/webpack.config.js",
    "build-server-prod": "echo \"TODO\"",
    "build-prod": "npm run build-client-prod && npm run build-server-prod",
    "watch-client": "webpack -d --watch --devtool source-map --config config/webpack.config.js",
    "watch-server": "npm run watch-init && concurrently \"npm run watch-tsc\" \"npm run watch-babel\" \"npm run watch-node\"",
    "watch-init": "npm run clean-server && npm run tsc && npm run babel",
    "watch-tsc": "tsc -w -p config/tsconfig.server.json",
    "watch-babel": "babel -w temp/server --out-dir server",
    "watch-node": "nodemon server/server",
    "tsc": "tsc -p config/tsconfig.server.json",
    "babel": "babel temp/server --out-dir server",
    "clean-client": "rimraf client",
    "clean-server": "rimraf server",
    "clean-temp": "rimraf temp",
    "clean": "npm run clean-client && npm run clean-server",
    "serve": "node server/main.js"
  },
